<!DOCTYPE html>
<html lang="en">
<head>
	<title> Connections Page </title>
	<link rel="stylesheet" type="text/css" href="assets/stylesheets/style.css">
	<meta charset="utf-8">
</head>
<body>
	<% if(userspecificnavFlag == 0) { %>
	<% include partials/header.ejs %>
	<% include partials/navigation.ejs %>
	<% } else if (userspecificnavFlag == 1) { %>
	<% include partials/header2.ejs %>
	<% include partials/navigation2.ejs %>
	<% } %>
	<main>
			<% allCategories.forEach(function(category){ %>
				<h4><%= category %></h4>
				<div >
					<ul>
					<% allConnections.forEach(function(connectionData){ %>
						<% if(connectionData._category == category){ %>
						<li><a href="/Connection?id=<%= connectionData._connectionID %>" class="linkstyle1"> <%= connectionData._connectionName %> </a></li>
						<% } %>
					<% }); %>
				</ul>
			</div>
			<% }); %>
		<p>&nbsp;</p> <p>&nbsp;</p> <p>&nbsp;</p> <p>&nbsp;</p>
	</main>
  <% include partials/footer.ejs %>
 </body>
</html>
